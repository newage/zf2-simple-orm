{
  "name": "newage/zf2-simple-orm",
  "description": "",
  "license": "MIT",
  "keywords": [],
  "require": {
    "php": ">=5.4",
    "doctrine/common": ">=2.1",
    "zendframework/zendframework": ">=2.2",
    "newage/annotations": "master-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Newage\\SimpleOrm\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ExampleModel\\": "examples"
    }
  },
  "scripts": {
    "cs": "php vendor/bin/phpcs src --standard=PSR2 --colors",
    "cs-fix": "php ventor/bin/phpcbf src --standard=PSR2 --colors"
  }
}